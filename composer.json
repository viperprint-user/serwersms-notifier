{
    "name": "viperprint/serwersms-notifier",
    "description": "Symfony Serwersms Notifier Bridge",
    "keywords": ["sms","serwersms", "notifier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Lasota",
            "email": "piotr.lasota@viperprint.pl"
        }
    ],
    "autoload": {
        "psr-4": { "Symfony\\Component\\Notifier\\Bridge\\Serwersms\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "symfony/http-client": "^4.3|^5.0",
        "symfony/notifier": "^5.0"
    },
    "require-dev": {
        "symfony/event-dispatcher": "^4.3|^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
